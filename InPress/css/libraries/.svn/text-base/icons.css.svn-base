		url('../fonts/icons/icons.ttf') format('truetype'),
		url('../fonts/icons/icons.svg#icons') format('svg');
	font-weight: normal;
	font-style: normal;
}




/*Font face icons*/
[class^="icons-"], [class*=" icons-"]  {
  vertical-align: middle;
  margin-right:5px;
  margin-left:5px;
}


